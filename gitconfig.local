[user]
  name = John Regner
  email = john@johnregner.com

[alias]
  l = log --oneline --branches --decorate --graph
  s = status --short
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p

[help]
  autocorrect = 1

[color]
  ui = true
  wtf = true
